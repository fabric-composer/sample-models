{
  "devDependencies": {
    "colors": "^1.1.2",
    "lerna": "^2.0.0-rc.4",
    "license-check-and-add": "2.0.4",
    "moment": "^2.17.1",
    "semver": "^5.3.0",
    "sleep-promise": "^2.0.0"
  },
  "name": "sample-models",
  "description": "You must install [Lerna](https://lernajs.io) to build this multi-package repository.",
  "version": "0.2.2",
  "main": "index.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run pkgcheck && npm run bootstrap",
    "bootstrap": "lerna bootstrap",
    "licchk": "license-check-and-add",
    "test": "npm run licchk & lerna run test",
    "pkgcheck": "node ./scripts/pkgcheck.js",
    "pkgstamp": "node ./scripts/pkgstamp.js",
    "pkgbump": "node ./scripts/pkgbump.js && node ./scripts/pkgcheck.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer-sample-models.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "node_modules",
      "packages",
      ".git"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".md",
      ".yml",
      ".enc"
    ],
    "insert_license": false,
    "license_formats": {
      "js": {
        "prepend": "#!/usr/bin/env node\n/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      }
    }
  },
  "author": "Hyperledger Composer",
  "license": "Apache-2.0"
}
