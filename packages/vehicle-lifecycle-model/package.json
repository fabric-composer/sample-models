{
  "engines": {
    "composer": "^0.18.0"
  },
  "name": "vehicle-lifecycle-model",
  "version": "0.2.2",
  "description": "Vehicle Lifecycle Model",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer-sample-models.git"
  },
  "keywords": [
    "composer",
    "composer-model",
    "vehicle",
    "manufacturer",
    "regulator",
    "dealership",
    "auction house",
    "scrap merchant"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "scripts": {
    "licchk": "license-check-and-add",
    "test": "npm run licchk"
  },
  "devDependencies": {
    "license-check-and-add": "2.0.4"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "node_modules"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".md"
    ],
    "insert_license": false,
    "license_formats": {
      "cto": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      }
    }
  }
}
