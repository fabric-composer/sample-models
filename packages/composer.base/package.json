{
  "engines": {
    "composer": "^0.9.0"
  },
  "name": "composer.base",
  "version": "0.0.18",
  "description": "Composer Base Model",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer-sample-models.git"
  },
  "keywords": [
    "composer",
    "composer-model",
    "gender",
    "contact details",
    "birth details",
    "death details",
    "address"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "scripts": {
    "licchk": "license-check",
    "test": "npm run licchk"
  },
  "devDependencies": {
    "license-check": "^1.1.5"
  },
  "license-check-config": {
    "src": [
      "models/**/*.cto"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  }
}
